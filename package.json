{
  "name": "@dz-web/finance-chart",
  "version": "1.0.0-alpha.42",
  "description": "finance chart",
  "main": "index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "dist": "npm run clean && webpack --config ./dev/webpack.config.js",
    "example": "rimraf example-dist/ && webpack --config ./dev/webpack.example.js",
    "start": "node ./dev/serve.js",
    "test": "node ./dev/jest/start.js",
    "test:coverage": "npm run test -- --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "tslint": "tslint -c tslint.json 'example/**/*.ts' 'src/**/*.ts'",
    "precommit": "",
    "precommit2": "npm run tslint"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "yeluoqiuzhi",
  "license": "LGPL",
  "bugs": {
    "url": "https://github.com/gitpadtech/finance-chart/issues"
  },
  "homepage": "https://github.com/gitpadtech/finance-chart",
  "devDependencies": {
    "@material-ui/core": "^1.4.0",
    "@types/jest": "^22.2.3",
    "autoprefixer": "^8.2.0",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "jest": "^22.4.3",
    "node-sass": "^4.13.1",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.3",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.13.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.4",
    "ts-loader": "^4.2.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^2.0.2",
    "whatwg-fetch": "^2.0.4"
  },
  "dependencies": {
    "@types/d3-array": "^1.2.1",
    "@types/d3-scale": "^2.0.0",
    "@types/d3-shape": "^1.2.2",
    "@types/detect-it": "^2.1.0",
    "core-js": "^2.5.5",
    "d3-array": "^1.2.1",
    "d3-scale": "^2.0.0",
    "d3-shape": "^1.2.0",
    "detect-it": "^3.0.5",
    "husky": "^0.14.3",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0",
    "lodash.clamp": "^4.0.3",
    "lodash.uniq": "^4.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/dev/jest/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/style-mock.js"
    },
    "testMatch": [
      "<rootDir>/**/__tests__/**/?(*.)(spec|test).{js,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
